generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Transaction {
  id       String    @id
  idOwner  String
  checkin  DateTime
  checkout DateTime?
  plate          String
  name           String
  tel            String
  active   Boolean @default(true)
}